{
    "name": "crispercode/framework",
    "description": "PHP framework with encryption, authentication, and entity management",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "slim/slim": "^4.14",
        "slim/twig-view": "^3.4",
        "slim/csrf": "^1.5",
        "slim/psr7": "^1.8",
        "php-di/slim-bridge": "^3.4",
        "twig/twig": "^3.18",
        "twig/intl-extra": "^3.19",
        "sergeytsalkov/meekrodb": "^3.1",
        "monolog/monolog": "^3.9",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2",
        "halaxa/json-machine": "^1.2",
        "aws/aws-sdk-php": "^3.300",
        "symfony/console": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^3.10",
        "rector/rector": "^2.0",
        "symfony/translation-contracts": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "CrisperCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\CrisperCode\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "chrishksang"
        }
    ],
    "minimum-stability": "stable"
}
